<!DOCTYPE html>
<html>
<head>
  <title>VM Tester</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%# javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render partial: 'layouts/header' %>

<% if notice %>
    <div class="notice"><%= notice %></div>
<% end %>
<% if alert %>
    <div class="alert"><%= alert %></div>
<% end %>

<div class="content">
  <%= yield %>
</div>

</body>
</html>
